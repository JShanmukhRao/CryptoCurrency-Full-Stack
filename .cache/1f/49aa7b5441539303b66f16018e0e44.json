omponents/Block.js","original":{"line":37,"column":34}},{"generated":{"line":97,"column":0},"source":"components/Block.js","original":{"line":37,"column":12}},{"generated":{"line":97,"column":21},"source":"components/Block.js","original":{"line":34,"column":8}},{"generated":{"line":97,"column":22},"source":"components/Block.js","original":{"line":33,"column":8}},{"generated":{"line":98,"column":0},"source":"components/Block.js","original":{"line":40,"column":5}},{"name":"Component","generated":{"line":102,"column":2},"source":"components/Block.js","original":{"line":3,"column":20}},{"name":"Block","generated":{"line":104,"column":15},"source":"components/Block.js","original":{"line":54,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\r\nimport Transaction from './Transaction'\r\nclass Block extends Component {\r\n    state = { displayTransaction: false };\r\n    toggleTransaction = () => {\r\n        this.setState({ displayTransaction: !this.state.displayTransaction });\r\n    }\r\n\r\n    get displayTransaction() {\r\n\r\n        const { data } = this.props.block;\r\n\r\n        const stringifyData = JSON.stringify(data);\r\n        const dataDisplay = stringifyData.length > 35 ? `${stringifyData.substring(0, 35)}...` : stringifyData;\r\n\r\n        if(this.state.displayTransaction)\r\n        {\r\n            return (\r\n                <div>\r\n                    Data: {\r\n                        data.map(transaction=>(\r\n                            <div key={transaction.id}>\r\n                                <hr />\r\n                                <Transaction transaction={transaction}   />\r\n                            </div>\r\n                        ))\r\n                    }\r\n                    <br/>\r\n                    <button onClick={this.toggleTransaction}>Show Less</button>\r\n                    </div>\r\n                    )\r\n        }\r\n        return (\r\n        <div>\r\n            Data: {dataDisplay}\r\n            <br/>\r\n            <button onClick={this.toggleTransaction}>Show More</button>\r\n            </div>\r\n            )\r\n    }\r\n    render() {\r\n        const { timestamp, hash } = this.props.block;\r\n        const hashDisplay = `${hash.substring(0, 15)}...`;\r\n\r\n        return (\r\n            <div className=\"Block\">\r\n                <div>Hash: {hashDisplay}</div>\r\n                <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\r\n                {this.displayTransaction}\r\n            </div>\r\n        );\r\n    }\r\n}\r\nexport default Block"},"lineCount":null}},"hash":"c362a2319a531de63296099a03a39a40","cacheData":{"env":{}}}